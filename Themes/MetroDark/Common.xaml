<ResourceDictionary
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
  mc:Ignorable="d"
  xmlns:d="http://schemas.microsoft.com/expression/blend/2008">

    <Color x:Key="HoverColor">#828282</Color>
    <SolidColorBrush x:Key="LightBrush" Color="#FF333333" />
    <Color x:Key="WhiteColor">#FFFFFFFF</Color>

    <SolidColorBrush x:Key="ControlBackgroundBrush" Color="#000000" />
    <SolidColorBrush x:Key="SubMenuBackgroundBrush" Color="#606060" />
    <SolidColorBrush x:Key="NormalBorderBrush" Color="#646464" />

    <SolidColorBrush x:Key="DisabledForegroundBrush" Color="#888" />
    <SolidColorBrush x:Key="DisabledBackgroundBrush" Color="#FF787878" />
    <SolidColorBrush x:Key="DisabledBorderBrush" Color="#AAA" />

    <SolidColorBrush x:Key="HoverBrush" Color="{StaticResource HoverColor}" />
    <SolidColorBrush x:Key="TextBrush" Color="#AAAAAA" />

    <SolidColorBrush x:Key="BaseBackgroundColor" Color="#000000"/>
</ResourceDictionary>
